{
  "name": "qs-jwt-docs",
  "version": "1.0.0",
  "description": "Documentation site for qs-jwt",
  "main": "index.js",
  "scripts": {
    "pre:version": "node ./scripts/fetch-qsjwt-version.mjs",
    "docs:dev": "npm run pre:version && vitepress dev ./docs",
    "docs:build": "npm run pre:version && vitepress build ./docs",
    "docs:preview": "npm run pre:version && vitepress preview ./docs",
    "deploy": "npm run docs:build && gh-pages -d docs/.vitepress/dist",
    "deploy:ci": "npm run docs:build"
  },
  "keywords": [
    "qlik-sense",
    "jwt",
    "authentication",
    "documentation"
  ],
  "author": "Ptarmigan Labs",
  "license": "MIT",
  "devDependencies": {
    "gh-pages": "^6.3.0",
    "ufo": "^1.6.1",
    "vitepress": "^1.6.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ptarmiganlabs/qs-jwt-docs.git"
  },
  "bugs": {
    "url": "https://github.com/ptarmiganlabs/qs-jwt-docs/issues"
  },
  "homepage": "https://qs-jwt.ptarmiganlabs.com/"
}
